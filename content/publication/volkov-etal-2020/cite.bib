@article{Volkov-etal-2020,
 abstract = {The Atlantic meridional overturning circulation (AMOC) is a key component of the ocean circulation system that is constantly moving water, heat, salt, carbon, nutrients, and other substances around the globe. The AMOC impacts the Atlantic Ocean in a unique way, making it the only ocean basin where heat is carried northward in both hemispheres.},
 author = {D. L. Volkov and C. S. Meinen and others},
 doi = {10.1175/BAMS-D-20-0105.1},
 journal = {Bull. Amer. Meteor. Soc.},
 pages = {159--163},
 title = {Atlantic meridional overturning circulation and associated heat transport},
 url = {https://nora.nerc.ac.uk/id/eprint/528362},
 year = {2020}
}

